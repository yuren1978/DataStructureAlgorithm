{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.associations": {
        "vector": "cpp",
        "iosfwd": "cpp"
    },
    "code-runner.executorMap": {

        "cpp":   "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "csharp": "scriptcs -script "
    }
}